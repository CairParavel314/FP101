Algoritmo EjemploOperadores
    // Declaración de variables
    Definir a, b Como Entero
    Definir resultadoNumerico Como Real
    Definir esVerdadero Como Logico
    Definir texto1, texto2 Como Cadena
	
    // Asignación inicial de valores
    a <- 10
    b <- 3
    texto1 <- "Hola"
    texto2 <- "Mundo"
	
    // Operadores aritméticos
    Escribir "Operadores aritméticos:"
    Escribir "Suma: ", a, " + ", b, " = ", a + b
    Escribir "Resta: ", a, " - ", b, " = ", a - b
    Escribir "Multiplicación: ", a, " * ", b, " = ", a * b
    Escribir "División: ", a, " / ", b, " = ", a / b
	Escribir "Potencia: ", a, " ^ ", b, " = ", a ^ b
	Escribir "Raíz: ", a, " = ",  raiz(a)
    Escribir "Módulo (resto de división): ", a, " % ", b, " = ", a % b
	Escribir ""
	
    // Operadores relacionales
    Escribir "Operadores relacionales:"
    Escribir "¿a es mayor que b? ", a, " > ", b, " = ", a > b
    Escribir "¿a es menor que b? ", a, " < ", b, " = ", a < b
    Escribir "¿a es igual a b? ", a, " = ", b, " = ", a = b
    Escribir "¿a es diferente de b? ", a, " != ", b, " = ", a <> b
    Escribir "¿a es mayor o igual que b? ", a, " >= ", b, " = ", a >= b
    Escribir "¿a es menor o igual que b? ", a, " <= ", b, " = ", a <= b
	Escribir ""
	
    // Operadores lógicos
    Definir valor1, valor2 Como Logico
    valor1 <- Verdadero
    valor2 <- Falso
	
    Escribir "Operadores lógicos:"
    Escribir "AND (valor1 Y valor2): ", valor1, " Y ", valor2, " = ", valor1 Y valor2
    Escribir "OR (valor1 O valor2): ", valor1, " O ", valor2, " = ", valor1 O valor2
    Escribir "NOT (NO valor1): ", "NO ", valor1, " = ", NO valor1
	Escribir ""
	
    // Operador de concatenación
    Escribir "Operador de concatenación:"
    Escribir "Concatenación de cadenas: ", texto1, " + ", texto2, " = ", texto1 , " " , texto2

FinAlgoritmo

